<div id="documentsDiv">
<table cellpadding="0" cellspacing="0" border="0" class="display" id="documentsTableID">
  <thead>
    <tr>
      <th style="width: 490px">Content</th>
      <th style="width: 100px">_key</th>
      <th style="width: 10px"></th>
